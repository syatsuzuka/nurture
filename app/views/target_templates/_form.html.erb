<%= simple_form_for [@target_templates_set, @target_template] do |f| %>
  <%= f.input :name, required: true %>
  <%= f.input :description, required: true %>
  <%= f.input :score, required: true %>
  <%= f.submit 'Save', class: "btn btn-success text-light" %>
  <%= link_to 'Cancel', target_templates_set_target_templates_path(@target_templates_set), class: 'btn btn-secondary text-light'%>
<% end %>
