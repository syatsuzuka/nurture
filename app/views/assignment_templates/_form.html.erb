<%= simple_form_for [@assignment_templates_set, @assignment_template] do |f| %>
  <%= f.input :title, required: true %>
  <%= f.input :instruction, required: true %>
  <%= f.input :instruction_url %>
  <%= f.input :checkpoint, required: true %>
  <%= f.submit 'Save', class: "btn btn-success text-light" %>
  <%= link_to 'Cancel', assignment_templates_set_assignment_templates_path(@assignment_templates_set), class: 'btn btn-secondary text-light'%>
<% end %>
